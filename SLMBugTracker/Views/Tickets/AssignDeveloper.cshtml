@model SLMBugTracker.Models.ViewModels.AssignDeveloperViewModel

<div class="container">
    <h2>Assign Developer for</h2>
    <div class="row">

        <div class="col-md-6">

            <h3>@Model.Ticket.Title</h3>



            <div class="card">
                <div class="card-header">
                    <label class="control-label">Assign Developer</label>
                </div>
                <div class="card-body">
                    <form method="post" asp-action="AssignDeveloper" asp-controller="Tickets">
                    
                    </form>
                </div>
            </div>
        </div>
    </div>

    <div>
        <a asp-action="UnassignedTickets" asp-controller="Tickets">Unassigned Tickets</a>
    </div>

</div>